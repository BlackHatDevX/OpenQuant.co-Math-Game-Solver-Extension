{
  "manifest_version": 3,
  "name": "OpenQuant Math Game Solver",
  "version": "1.0",
  "description": "Automatically solves arithmetic problems on OpenQuant math game",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://openquant.co/math-game*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 